le reset for older browsers */\narticle, aside, details, figcaption, figure, \nfooter, header, hgroup, menu, nav, section {\n\tdisplay: block;\n}\nbody {\n\tline-height: 1;\n}\nol, ul {\n\tlist-style: none;\n}\nblockquote, q {\n\tquotes: none;\n}\nblockquote:before, blockquote:after,\nq:before, q:after {\n\tcontent: '';\n\tcontent: none;\n}\ntable {\n\tborder-collapse: collapse;\n\tborder-spacing: 0;\n}","build/css/style.css":"/* http://meyerweb.com/eric/tools/css/reset/ \n   v2.0 | 20110126\n   License: none (public domain)\n*/\n@import url(/fonts/fontstyle.css);\nhtml, body {\n  box-sizing: border-box; }\n\nhtml, body, div, span, applet, object, iframe,\nh1, h2, h3, h4, h5, h6, p, blockquote, pre,\na, abbr, acronym, address, big, cite, code,\ndel, dfn, em, img, ins, kbd, q, s, samp,\nsmall, strike, strong, sub, sup, tt, var,\nb, u, i, center,\ndl, dt, dd, ol, ul, li,\nfieldset, form, label, legend,\ntable, caption, tbody, tfoot, thead, tr, th, td,\narticle, aside, canvas, details, embed,\nfigure, figcaption, footer, header, hgroup,\nmenu, nav, output, ruby, section, summary,\ntime, mark, audio, video {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  font-size: 100%;\n  font: inherit;\n  vertical-align: baseline; }\n\n/* HTML5 display-role reset for older browsers */\narticle, aside, details, figcaption, figure,\nfooter, header, hgroup, menu, nav, section {\n  display: block; }\n\nbody {\n  line-height: 1; }\n\nol, ul {\n  list-style: none; }\n\nblockquote, q {\n  quotes: none; }\n\nblockquote:before, blockquote:after,\nq:before, q:after {\n  content: '';\n  content: none; }\n\ntable {\n  border-collapse: collapse;\n  border-spacing: 0; }\n\nhtml, body {\n  font-family: 'Open Sans';\n  font-style: normal;\n  font-weight: 300;\n  background-color: #000000;\n  color: #FFFFFF;\n  box-sizing: border-box;\n  font-size: 16px;\n  max-width: 100%; }\n  html img, body img {\n    max-width: 100%;\n    max-height: 100%; }\n\n/* Header on-change classes */\n.new-size {\n  height: 200px; }\n  @media (min-width: 600px) {\n    .new-size {\n      height: 150px; } }\n  @media (min-width: 1240px) {\n    .new-size {\n      height: 150px; } }\n\n.new-logo {\n  width: 105px;\n  height: 105px;\n  padding: 20px 0 0 0; }\n  @media (min-width: 600px) {\n    .new-logo {\n      padding: 0 52.5px 0 0; } }\n  @media (min-width: 1240px) {\n    .new-logo {\n      padding: 0 52.5px 0 0; } }\n\n/* Header */\n.intro-webpage {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  transition: all 0.25s ease-in-out 0.25s; }\n  .intro-webpage.default-size {\n    height: 85vh; }\n  @media (min-width: 600px) {\n    .intro-webpage {\n      flex-direction: row; } }\n  @media (min-width: 1240px) {\n    .intro-webpage {\n      justify-content: flex-start;\n      padding-left: 75px; } }\n  .intro-webpage .nyt-logo.logo-size {\n    width: 225px;\n    height: 225px; }\n    @media (min-width: 600px) {\n      .intro-webpage .nyt-logo.logo-size {\n        width: 210px;\n        height: 210px;\n        padding: 0 30px 0 0; } }\n    @media (min-width: 1240px) {\n      .intro-webpage .nyt-logo.logo-size {\n        width: 225px;\n        height: 225px; } }\n  .intro-webpage p {\n    padding: 15px 0; }\n  .intro-webpage .selector {\n    text-align: center;\n    padding-bottom: 15px; }\n    @media (min-width: 600px) {\n      .intro-webpage .selector {\n        text-align: left; } }\n    .intro-webpage .selector .dropdown-section {\n      border-radius: 5px;\n      border: none;\n      text-align: center; }\n  .intro-webpage .preload {\n    display: none;\n    align-self: center;\n    width: 37.5px;\n    height: 37.5px;\n    padding-top: 15px; }\n    @media (min-width: 600px) {\n      .intro-webpage .preload {\n        align-self: center; } }\n\n/* Body */\nsection .news-content {\n  width: 100%;\n  display: grid;\n  grid-template-columns: repeat(1, 1fr);\n  grid-template-rows: repeat(12, 1fr); }\n  @media (min-width: 600px) {\n    section .news-content {\n      grid-template-columns: repeat(3, 1fr);\n      grid-template-rows: repeat(4, 1fr); } }\n  @media (min-width: 1240px) {\n    section .news-content {\n      grid-template-columns: repeat(4, 1fr);\n      grid-template-rows: repeat(3, 1fr); } }\n\nli a, li a:link, li a:visited {\n  text-decoration: none;\n  color: #FFFFFF; }\n  li a article, li a:link article, li a:visited article {\n    display: flex;\n    flex-direction: column;\n    justify-content: flex-end;\n    width: 100%;\n    height: 350px;\n    background-position: center;\n    background-size: cover;\n    background-repeat: no-repeat; }\n    @media (min-width: 600px) {\n      li a article, li a:link article, li a:visited article {\n        width: 100%;\n        height: 450px; } }\n    @media (min-width: 1240px) {\n      li a article, li a:link article, li a:visited article {\n        width: 100%;\n        height: 430px; } }\n    li a article .text-content, li a:link article .text-content, li a:visited article .text-content {\n      height: auto;\n      background-color: rgba(0, 0, 0, 0.5);\n      padding: 10px 5px;\n      line-height: 25px; }\n\n/* Footer */\nfooter {\n  color: #C2C2C2;\n  text-align: center; }\n  @media (min-width: 1240px) {\n    footer {\n      text-align: left;\n      padding-left: 75px; } }\n  footer p {\n    padding: 37.5px 0; }\n","build/css/style.scss":"@import 'reset';\n@import '/fonts/fontstyle.css';\n@import 'variables';\n@import 'mixin';\n@import 'global';\n@import 'header';\n@import 'section';\n@import 'footer';"},"lineCount":null}},"error":null,"hash":"256c856556d6dc56f25ca1e85e67a288","cacheData":{"env":{}}}