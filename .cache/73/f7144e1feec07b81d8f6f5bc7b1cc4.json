v, output, ruby, section, summary,\ntime, mark, audio, video {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  font-size: 100%;\n  font: inherit;\n  vertical-align: baseline; }\n\n/* HTML5 display-role reset for older browsers */\narticle, aside, details, figcaption, figure,\nfooter, header, hgroup, menu, nav, section {\n  display: block; }\n\nbody {\n  line-height: 1; }\n\nol, ul {\n  list-style: none; }\n\nblockquote, q {\n  quotes: none; }\n\nblockquote:before, blockquote:after,\nq:before, q:after {\n  content: '';\n  content: none; }\n\ntable {\n  border-collapse: collapse;\n  border-spacing: 0; }\n\nhtml, body {\n  font-family: 'Open Sans';\n  font-style: normal;\n  font-weight: 300;\n  background-color: #000000;\n  color: #FFFFFF;\n  box-sizing: border-box;\n  font-size: 16px;\n  max-width: 100%; }\n  html img, body img {\n    max-width: 100%;\n    max-height: 100%; }\n\n/* Header on-change classes */\n.new-size {\n  height: 200px; }\n  @media (min-width: 600px) {\n    .new-size {\n      height: 150px; } }\n  @media (min-width: 1240px) {\n    .new-size {\n      height: 150px; } }\n\n.new-logo {\n  width: 105px;\n  height: 105px;\n  padding: 20px 0 0 0; }\n  @media (min-width: 600px) {\n    .new-logo {\n      padding: 0 52.5px 0 0; } }\n  @media (min-width: 1240px) {\n    .new-logo {\n      padding: 0 52.5px 0 0; } }\n\n/* Header */\n.intro-webpage {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  transition: all 0.25s ease-in-out 0.25s; }\n  .intro-webpage.default-size {\n    height: 85vh; }\n  @media (min-width: 600px) {\n    .intro-webpage {\n      flex-direction: row; } }\n  @media (min-width: 1240px) {\n    .intro-webpage {\n      justify-content: flex-start;\n      padding-left: 75px; } }\n  .intro-webpage .nyt-logo.logo-size {\n    width: 225px;\n    height: 225px; }\n    @media (min-width: 600px) {\n      .intro-webpage .nyt-logo.logo-size {\n        width: 210px;\n        height: 210px;\n        padding: 0 30px 0 0; } }\n    @media (min-width: 1240px) {\n      .intro-webpage .nyt-logo.logo-size {\n        width: 225px;\n        height: 225px; } }\n  .intro-webpage p {\n    padding: 15px 0; }\n  .intro-webpage .selector {\n    text-align: center;\n    padding-bottom: 15px; }\n    @media (min-width: 600px) {\n      .intro-webpage .selector {\n        text-align: left; } }\n    .intro-webpage .selector .dropdown-section {\n      border-radius: 5px;\n      border: none;\n      text-align: center; }\n  .intro-webpage .preload {\n    display: none;\n    align-self: center;\n    width: 37.5px;\n    height: 37.5px;\n    padding-top: 15px; }\n    @media (min-width: 600px) {\n      .intro-webpage .preload {\n        align-self: center; } }\n\n/* Body */\nsection .news-content {\n  width: 100%;\n  display: grid;\n  grid-template-columns: repeat(1, 1fr);\n  grid-template-rows: repeat(12, 1fr); }\n  @media (min-width: 600px) {\n    section .news-content {\n      grid-template-columns: repeat(3, 1fr);\n      grid-template-rows: repeat(4, 1fr); } }\n  @media (min-width: 1240px) {\n    section .news-content {\n      grid-template-columns: repeat(4, 1fr);\n      grid-template-rows: repeat(3, 1fr); } }\n\nli a, li a:link, li a:visited {\n  text-decoration: none;\n  color: #FFFFFF; }\n  li a article, li a:link article, li a:visited article {\n    display: flex;\n    flex-direction: column;\n    justify-content: flex-end;\n    width: 100%;\n    height: 350px;\n    background-position: center;\n    background-size: cover;\n    background-repeat: no-repeat; }\n    @media (min-width: 600px) {\n      li a article, li a:link article, li a:visited article {\n        width: 100%;\n        height: 450px; } }\n    @media (min-width: 1240px) {\n      li a article, li a:link article, li a:visited article {\n        width: 100%;\n        height: 430px; } }\n    li a article .text-content, li a:link article .text-content, li a:visited article .text-content {\n      height: auto;\n      background-color: rgba(0, 0, 0, 0.5);\n      padding: 10px 5px;\n      line-height: 25px; }\n\n/* Footer */\nfooter {\n  color: #C2C2C2;\n  text-align: center; }\n  @media (min-width: 1240px) {\n    footer {\n      text-align: left;\n      padding-left: 75px; } }\n  footer p {\n    padding: 37.5px 0; }\n","build/css/style.scss":"@import 'reset';\n@import '/fonts/fontstyle.css';\n@import 'variables';\n@import 'mixin';\n@import 'global';\n@import 'header';\n@import 'section';\n@import 'footer';","build/css/_global.scss":"html, body {\n    font-family: 'Open Sans';\n    font-style: normal;\n    font-weight: 300;\n    background-color: $background;\n    color: $font-color;\n    box-sizing: border-box;\n    font-size: $font-size;\n    max-width: 100%;\n    img { \n      max-width: 100%;\n      max-height: 100%;\n    }\n  }\n\n  /* Header on-change classes */\n.new-size {\n  height: $header-mobile;\n  @include tablet {\n      height: $header-tablet;\n  }\n  @include desktop {\n      height: $header-desktop;\n  }\n}\n\n.new-logo {\n  width: $global * 7;\n  height: $global * 7;\n  padding: ($global + 5) 0 0 0;\n  @include tablet {\n      padding: 0 ($global * 3.5) 0 0;\n  }\n  @include desktop {\n      padding: 0 ($global * 3.5) 0 0;\n  }\n}","build/css/_variables.scss":"// Website generals\r\n$background: #000000;\r\n\r\n// Fonts\r\n$font-color: #FFFFFF;\r\n$footer-color: #C2C2C2;\r\n$font-size: 16px;\r\n\r\n// Units of measure\r\n$global: 15px;\r\n$full-size: 100vh;\r\n$image-width: 100%;\r\n$image-height: 350px;\r\n\r\n// Header sizing\r\n$header-mobile: 200px;\r\n$header-tablet: 150px;\r\n$header-desktop: 150px;\r\n\r\n// Breakpoints\r\n$tabletBreakpoint: 600px;\r\n$desktopBreakpoint: 1240px;","build/css/_mixin.scss":"// Tablet\r\n@mixin tablet {\r\n    @media (min-width: $tabletBreakpoint) {\r\n        @content;\r\n    }\r\n}\r\n// Desktop\r\n@mixin desktop {\r\n    @media (min-width: $desktopBreakpoint) {\r\n        @content;\r\n    }\r\n}","build/css/_header.scss":"/* Header */\r\n.intro-webpage {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    transition: all 0.25s ease-in-out 0.25s;\r\n    &.default-size {\r\n      height: $full-size - 15;\r\n    }\r\n    @include tablet {\r\n      flex-direction: row;\r\n    }\r\n    @include desktop {\r\n      justify-content: flex-start;\r\n      padding-left: $global * 5;\r\n    }\r\n    .nyt-logo {\r\n      &.logo-size {\r\n      width: $global * 15;\r\n      height: $global * 15;\r\n      @include tablet {\r\n        width: $global * 14;\r\n        height: $global * 14;\r\n        padding: 0 ($global * 2) 0 0;\r\n      }\r\n      @include desktop {\r\n        width: $global * 15;\r\n        height: $global * 15;\r\n      }\r\n    }\r\n    }\r\n    p {\r\n      padding: $global 0;\r\n    }\r\n    .selector {\r\n      text-align: center;\r\n      padding-bottom: 15px;\r\n      @include tablet {\r\n        text-align: left;\r\n      }\r\n      .dropdown-section {\r\n        border-radius: 5px;\r\n        border: none;\r\n        text-align: center;\r\n    }}\r\n    .preload {\r\n      display: none;\r\n      align-self: center;\r\n      width: $global * 2.5;\r\n      height: $global * 2.5;\r\n      padding-top: $global;\r\n      @include tablet {\r\n        align-self: center;\r\n      }\r\n    }\r\n  }"},"lineCount":null}},"error":null,"hash":"4c2110c203b88416f8274054f2bfbb89","cacheData":{"env":{}}}